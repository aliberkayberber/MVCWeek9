@model AuthorDetailsViewModel

<h3>Author Details</h3>

<table class="table">
        <thead>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Birthday</th>

        </thead>
    <tbody>

        <tr >
            <td>
                @Model.Id
            </td>
            <td>
                @Model.FirstName
            </td>
            <td>
                @Model.LastName
            </td>
            <td>
                @Model.DateOfBirth
            </td>

        </tr>

    </tbody>
</table>